<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mappers">

	<select id="selectAllUser" resultType="com.munggu.org.User">
		select * from user;
	</select>
	
	<select id="selectOneUser" resultType="com.munggu.org.User">
		select * from user where idx='1';
	</select>
	    
</mapper>